{
  "interpreters": {
    "javascript": [
      [ "config/", "bin/", "server.js",
        {
          "default": "require",
          "exclude_paths": [
            "src"
            ".git",
            "*.log",
            "node_modules"
          ]
        }
      ],
      [ "src",
        {
          "default": "import",
          "exclude_paths": [
            "server"
            ".git",
            "*.log",
            "node_modules"
          ]
        }
      ]
    ],
  },
  "scss": {
    "path": "src/static"
  }
}
