{
  "javascript": {
    "syntax": "javascript",
    "handlers" : {
      "import": {
        "default": true,
        "match": [
          "import {variable}",
          "import {variable} from {module}"
        ],
        "result": "import {variable} from \"{module}\""
      },
      "require": {
        "match": [
          "(const|let|var) {variable}",
          "(const|let|var) {variable} = {module}"
        ],
        "result": "const {variable} = require(\"{module}\")"
      }
    }
  },
  "less": {
    "syntax": "less",
    "handlers": {
      "import": {
        "default": true,
        "match": [
          "@import {module}"
        ],
        "result": "@import {\"module}\""
      }
    }
  }
}
